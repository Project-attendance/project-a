<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <!-- UIkit CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/uikit@3.7.1/dist/css/uikit.min.css" />

    <!-- UIkit JS -->
    <script src="https://cdn.jsdelivr.net/npm/uikit@3.7.1/dist/js/uikit.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/uikit@3.7.1/dist/js/uikit-icons.min.js"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1">

</head>

<body>
    <style>
        form {
            width: 100%;
            margin: 4%;
        }

        .box {
            display: flex;
            justify-content: center;
            align-items: center;
        }
    </style>
    <div class="box">
        <form class="uk-form-horizontal uk-margin-large" action="/TimeTable-api" method="post">
            <h3><b>craet timetable</b></h3>
            <hr>
            <div class="uk-margin">
                <label class="uk-form-label" for="form-horizontal-text">Enter your name</label>
                <div class="uk-form-controls">
                    <input class="uk-input" id="form-horizontal-text" type="text" name="name" placeholder="enter name">
                </div>
            </div>
            <div class="uk-margin">
                <label class="uk-form-label" for="form-horizontal-text">Enter barch </label>
                <div class="uk-form-controls">
                    <input class="uk-input" id="form-horizontal-text" type="text" name="branch"
                        placeholder="enter batch">
                </div>
            </div>
            <div>
                <ul uk-tab>


                    <li><a href="#" class="uk-active">Time</a></li>
                    <li><a href="#">Lectures</a></li>
                    <li><a href="#">Monday</a></li>
                    <li><a href="#">Tuesday</a></li>
                    <li><a href="#">Wednesday</a></li>
                    <li><a href="#">Thursday</a></li>
                    <li><a href="#">Friday</a></li>
                    <li><a href="#">Saturday</a></li>

                </ul>
                <ul class="uk-switcher uk-margin">
                    <li>
                        <% for(var i=0;i<6;i++){%>
                            <div>
                                <div class="uk-width-1-2@s">
                                    <label class="uk-form-label" for="form-horizontal-text">Enter start time</label>
                                    <div class="uk-form-controls">
                                        <input type="time" class="uk-input" name="st<%= i+1 %>">
                                    </div>
                                </div>
                                <br>
                                <div class="uk-width-1-2@s">
                                    <label class="uk-form-label" for="form-horizontal-text">Enter end time</label>
                                    <div class="uk-form-controls">
                                        <input type="time" class="uk-input" name="et<%= i+1 %>">
                                    </div>
                                </div>
                            </div>
                            <hr>
                            <% } %>
                    </li>

                    <li>
                        <% for(var k=0; k< 9;k++){%>
                            <div class="uk-grid" uk-grid>

                                <div class="uk-width-1-3@s">
                                    <div class="uk-inline">
                                        <a class="uk-form-icon uk-form-icon-flip" href="#" uk-icon="icon: pencil"></a>
                                        <input class="uk-input" type="text" placeholder="lecturer" name="n<%=k+1%>">
                                    </div>
                                </div>

                                <div class="uk-width-1-3@s">
                                    <div class="uk-inline">
                                        <a class="uk-form-icon uk-form-icon-flip" href="#" uk-icon="icon: pencil"></a>
                                        <input class="uk-input" type="text" placeholder="subject" name="sn<%=k+1%>">
                                    </div>
                                </div>
                                <div class="uk-width-1-3@s">
                                    <div class="uk-inline">
                                        <a class="uk-form-icon uk-form-icon-flip" href="#" uk-icon="icon: link"></a>
                                        <input class="uk-input" type="text" placeholder="link" name="l<%=k+1%>">
                                    </div>
                                </div>
                            </div>
                            <br>
                            <div class="uk-width-1-1@s">
                                <div class="uk-margin">
                                    <textarea class="uk-textarea" rows="5" placeholder="Discription"
                                        name="d<%=k+1 %>"></textarea>
                                </div>

                            </div>
                            <strong>
                                <hr>
                            </strong>

                            <% } %>
                    </li>
                    <li>



                        <!-- subjects and lines -->
                        <div class="uk-margin">
                            <% for(var i=0; i<6;i++) { %>
                                <div class="uk-grid-small" uk-grid>
                                    <div class="uk-width-1@s">
                                        <label class="uk-form-label" for="form-horizontal-text">Subject Name

                                        </label>
                                        <div class="uk-form-controls">
                                            <input class="uk-input" id="form-horizontal-text" type="text"
                                                name="s<%= i+1%>" placeholder="Some text...">
                                        </div>
                                    </div>

                                </div>
                                <% } %>
                        </div>

                    </li>
                    <li>
                        <!-- set timing -->




                        <!-- subjects and lines -->
                        <div class="uk-margin">
                            <% for(var i=0; i<6;i++) { %>
                                <div class="uk-grid-small" uk-grid>
                                    <div class="uk-width-1@s">
                                        <label class="uk-form-label" for="form-horizontal-text">Subject Name

                                        </label>
                                        <div class="uk-form-controls">
                                            <input class="uk-input" id="form-horizontal-text" type="text"
                                                name="s1<%= i+1%>" placeholder="Some text...">
                                        </div>
                                    </div>

                                </div>
                                <% } %>
                        </div>

                    </li>
                    <li>
                        <!-- set timing -->




                        <!-- subjects and lines -->
                        <div class="uk-margin">
                            <% for(var i=0; i<6;i++) { %>
                                <div class="uk-grid-small" uk-grid>
                                    <div class="uk-width-1@s">
                                        <label class="uk-form-label" for="form-horizontal-text">Subject Name

                                        </label>
                                        <div class="uk-form-controls">
                                            <input class="uk-input" id="form-horizontal-text" type="text"
                                                name="s2<%= i+1%>" placeholder="Some text...">
                                        </div>
                                    </div>
                                </div>
                                <% } %>
                        </div>

                    </li>
                    <li>
                        <!-- set timing -->




                        <!-- subjects and lines -->
                        <div class="uk-margin">
                            <% for(var i=0; i<6;i++) { %>
                                <div class="uk-grid-small" uk-grid>
                                    <div class="uk-width-1-1@s">
                                        <label class="uk-form-label" for="form-horizontal-text">Subject Name

                                        </label>
                                        <div class="uk-form-controls">
                                            <input class="uk-input" id="form-horizontal-text" type="text"
                                                name="s3<%= i+1%>" placeholder="Some text......">
                                        </div>

                                    </div>
                                    <% } %>
                                </div>

                    </li>

                    <li>
                        <!-- set timing -->




                        <!-- subjects and lines -->
                        <div class="uk-margin">
                            <% for(var i=0; i<6;i++) { %>
                                <div class="uk-grid-small" uk-grid>
                                    <div class="uk-width-1@s">
                                        <label class="uk-form-label" for="form-horizontal-text">Subject Name

                                        </label>
                                        <div class="uk-form-controls">
                                            <input class="uk-input" id="form-horizontal-text" type="text"
                                                name="s4<%= i+1%>" placeholder="Some text...">
                                        </div>
                                    </div>

                                </div>
                                <% } %>
                        </div>

                    </li>
                    <li>
                        <!-- set timing -->




                        <!-- subjects and lines -->
                        <div class="uk-margin">
                            <% for(var i=0; i<6;i++) { %>
                                <div class="uk-grid-small" uk-grid>
                                    <div class="uk-width-1@s">
                                        <label class="uk-form-label" for="form-horizontal-text">Subject Name

                                        </label>
                                        <div class="uk-form-controls">
                                            <input class="uk-input" id="form-horizontal-text" type="text"
                                                name="s5<%= i+1%>" placeholder="Some text...">
                                        </div>
                                    </div>

                                </div>
                                <% } %>
                        </div>

                    </li>



                </ul>

            </div>

            <button class="btn btn-primary" type="submit">submit</button>
        </form>


    </div>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.0/jquery.min.js">
    </script>
